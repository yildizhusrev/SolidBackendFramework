@model TmTFramework.Entities.ComplexType.ServisDetails




@{
    string hidden = "";
    ViewBag.Title = "Servis Kayıt";
    if (IsAjax)
    {
        Layout = null;
        hidden = "hidden";
    }
}

<section class="content-header @hidden">
    <h1>
        Servis Kayıt
    </h1>
</section>

@using (Ajax.BeginForm(null, null, new AjaxOptions { HttpMethod = "POST", OnFailure = "Hatali", OnSuccess = "Basarili" }, new { id = "formKapsayici" }))
{

    @Html.AntiForgeryToken()
    <section class="content">
        <div class="box box-default">
            <div class="box-body">
                <div class="row">
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

                    <div class="col-md-6">
                        <div class="form-group">
                            @Html.LabelFor(model => model.ServisZamani, "MusteriId", htmlAttributes: new { @class = "control-label " })
                            @Html.EditorFor(model => model.ServisZamani, new { htmlAttributes = new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.ServisZamani) } })
                            @Html.ValidationMessageFor(model => model.ServisZamani, "", new { @class = "text-danger" })
                        </div>
                        <div class="form-group">
                            @Html.LabelFor(model => model.AracId, "AracId", htmlAttributes: new { @class = "control-label" })
                            @Html.DropDownList("AracId", null, htmlAttributes: new { @class = "form-control", placeholder = Html.DisplayNameFor(x => x.AracId) })
                            @Html.ValidationMessageFor(model => model.AracId, "", new { @class = "text-danger" })
                        </div>

                    </div>
                    <div class="col-md-6">



                    </div>
                </div>
            </div>
        </div>
        <div class="box-footer">

            @Html.Partial("_FormKaydetIptalPartial", new ViewDataDictionary { { "SubmitButtonText", "Kaydet" } })
        </div>
    </section>
}
@Html.Partial("_PartialCRUDAjaxScriptResponse")
