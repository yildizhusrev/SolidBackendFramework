        function errorAlert(ex) {
            var data = ex;
            var start = data.indexOf("<title>");
            var stop = data.indexOf("</title>");
            var mesaj = data.substring(start + 7, stop);
            toastr.error(mesaj);
        }

        //ex xhr kullanımı
        function errorAlert2(ex) {
            var data = ex.responseText;
            var start = data.indexOf("<title>");
            var stop = data.indexOf("</title>");
            var mesaj = data.substring(start + 7, stop);
            toastr.error('Hata Kodu:' + ex.status + '.\n' + mesaj);
        }
